    "prefix": ["fc", "react-fc", "sfc", "react-sfc"],
    "body": [
      "import React from 'react'",
      "",
      "type ${1:name}Props = {",
      "${2:firstProp}: ${3:firstPropType}",
      "}",
      "",
      "const ${1:name}: React.FC<${1:name}Props> = ({ ${2:firstProp} }) => {",
      "return <>${1:name}</>",
      "}",
      "",
      "export default ${1:name}"
    ],
    "description": "A React stateless function component file"
  },
